<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="apps.kelly.lineupmanagerv10">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".HomePageActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddPlayerActivity"></activity>
        <activity android:name="PlayerListActivity"></activity>
        <activity android:name=".PlayerDetailActivity"></activity>
        <activity android:name=".AddTeamActivity"></activity>
        <activity android:name=".TeamListActivity"></activity>
        <activity android:name=".TeamDetailActivity"></activity>
        <activity android:name=".EditPlayerActivity"></activity>
    </application>

</manifest>